<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
    <settings defaultResolver="main" />
    <!--Authentication required for publishing (deployment). 'Artifactory Realm' is the realm used by Artifactory so
    don't change it.
    Add to the next line your username and password (username="username" passwd="password") if you need to publish the artifact to as-artifacts.org.-->
    <credentials host="as-artifacts.org" realm="Artifactory Realm" />
    <resolvers>
        <chain name="main">
            <ibiblio name="as-artifacts" m2compatible="true" root="http://as-artifacts.org/swiftsuspenders" />
        </chain>
    </resolvers>
</ivysettings>